
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_green"
    android:padding="16dip">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dip"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/alcohol_block">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/alcohol"
                    android:background="#00000000"
                    android:src="@drawable/alcohol"
                    android:layout_weight="0.7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Alcohol"
                    android:id="@+id/textView2"
                    style="@style/MyText"
                    android:textColor="#ffffff"/>
                </LinearLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true">
                <RatingBar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ratingBarAlcohol"
                    android:layout_alignParentEnd="true"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/rateNote1"
                    android:textColor="#ffffff"
                    android:layout_marginTop="-10dip"
                    android:layout_marginRight="5dip"
                    android:layout_below="@+id/ratingBarAlcohol"
                    android:layout_alignParentEnd="true"
                    style="@style/MyText"
                    />
                </RelativeLayout>
            </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/smoking_block">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/smoking"
                    android:background="#00000000"
                    android:src="@drawable/smoking"
                    android:layout_weight="0.7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Smoking"
                    android:id="@+id/textView20"
                    style="@style/MyText"
                    android:textColor="#ffffff"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true">
                <RatingBar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ratingBarSmoking"
                    android:layout_alignParentEnd="true"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/rateNote2"
                    android:textColor="#ffffff"
                    android:layout_marginTop="-10dip"
                    android:layout_marginRight="5dip"
                    android:layout_below="@+id/ratingBarSmoking"
                    android:layout_alignParentEnd="true"
                    style="@style/MyText"
                    />
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/overwork_block">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/overwork"
                    android:background="#00000000"
                    android:src="@drawable/overwork1"
                    android:layout_weight="0.7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Overwork"
                    android:id="@+id/textView21"
                    style="@style/MyText"
                    android:textColor="#ffffff"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true">
                <RatingBar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ratingBarOverwork"
                    android:layout_alignParentEnd="true"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/rateNote3"
                    android:textColor="#ffffff"
                    android:layout_marginTop="-10dip"
                    android:layout_marginRight="5dip"
                    android:layout_below="@+id/ratingBarOverwork"
                    android:layout_alignParentEnd="true"
                    style="@style/MyText"
                    />
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/eating_disorder_block">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/eating_disorder"
                    android:background="#00000000"
                    android:src="@drawable/eating_disorder1"
                    android:layout_weight="0.7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Eating Disorder"
                    android:id="@+id/textView23"
                    style="@style/MyText"
                    android:textColor="#ffffff"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true">
                <RatingBar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ratingBarEatingDisOrder"
                    android:layout_alignParentEnd="true"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/rateNote4"
                    android:textColor="#ffffff"
                    android:layout_marginTop="-10dip"
                    android:layout_marginRight="5dip"
                    android:layout_below="@+id/ratingBarEatingDisOrder"
                    android:layout_alignParentEnd="true"
                    style="@style/MyText"
                    />
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/stay_up_late_block">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/stay_up_late"
                    android:background="#00000000"
                    android:src="@drawable/stay_up_late1"

                    android:layout_weight="0.7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Stay Up Late"
                    android:id="@+id/textView25"
                    style="@style/MyText"
                    android:textColor="#ffffff"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true">
                <RatingBar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ratingBarStayUpLate"
                    android:layout_alignParentEnd="true"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/rateNote5"
                    android:textColor="#ffffff"
                    android:layout_marginTop="-10dip"
                    android:layout_marginRight="5dip"
                    android:layout_below="@+id/ratingBarStayUpLate"
                    android:layout_alignParentEnd="true"
                    style="@style/MyText"
                    />
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>